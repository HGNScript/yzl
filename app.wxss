/* tabbar */
.tabbarbox {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 110rpx;
  border-top: 1rpx solid #f0f0f0;
  display: flex;
  background: #fff;
  z-index: 9999;
}
.tabbarbox2 {
  width: 25%;
}
.tabbarimage {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin:  18rpx auto 0;
}
.tabbartitle {
  text-align: center;
  font-size: 22rpx;
  color: #bfbfbf;
  line-height: 1.8;
}
.tabbaractive {
  color: #19b4ed;
}
/**app.wxss**/

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    /* padding: 200rpx 0; */
    box-sizing: border-box;
}

/* hr间隔 */

.hr {
    width: 100%;
    height: 15rpx;
    background-color: #f8f8f8;
}

.hr2 {
    width: 100%;
    height: 30rpx;
    background-color: #f8f8f8;
}

.hr3 {
    height: 60rpx;
    width: 100%;
    background: #f8f8f8;
}

.hr4 {
    width: 100%;
    height: 18rpx;
    background-color: #f8f8f8;
}


/* 消除浮动 */

.remove {
    clear: both;
}

.model2 {
    width: 100%;
    height: 100vh;
    background: rgba(0, 0, 0, 0.3);
    position: fixed;
    top: 0;
    left: 0;
    animation: myfirst 1.5s;
    z-index: 1000;
}

/* @keyframes myfirst {
from {
opacity: 0;
}

to {
opacity: 1;
}
} */

.model2 .box {
    background: #fff;
    width: 70%;
    height: 24vh;
    border-radius: 10rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: block;
    margin: auto;
    box-shadow: 3rpx 3rpx 10rpx rgba(0, 0, 0, 0.1);
}

.model2 .box > .title {
    text-align: center;
    color: #666;
    font-size: 34rpx;
    line-height: 13vh;
    letter-spacing: 2rpx;
}

.model2 .box > .submit {
    width: 60%;
    font-size: 32rpx;
    background: #19b4ed;
    padding: 0rpx 20rpx;
    text-align: center;
    color: #fff;
    letter-spacing: 2rpx;
    display: block;
    margin: 0 auto;
    border-radius: 10rpx;
}


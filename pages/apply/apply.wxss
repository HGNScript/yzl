/* pages/apply/apply.wxss */
.apply {
  margin: 20rpx;
  border-bottom: 2rpx #e5e5e5 solid;
}
.apply:last-child {
  border-bottom: 0rpx;  
}
.apply > .title {
  display: flex;
}
.apply > .title > image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}
.apply > .title > text {
  color: #555;
  font-size: 30rpx;
}
.apply > .app {
  padding: 50rpx 0 40rpx;
  width: 33%;
  display: inline-block;
}
.apply > .app > navigator > image {
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin: 0 auto;
}
.apply > .app > navigator > .name {
  text-align: center;
  font-size: 28rpx;
  color: #666;
  line-height: 1.8;
}
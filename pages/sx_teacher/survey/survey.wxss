/* pages/sx_teacher/survey/survey.wxss */

page {
    padding-bottom: 130rpx;
}

.header, .chartbox, .information {
    padding: 0 20rpx 40rpx;
    position: relative;
}

.title1 {
    line-height: 100rpx;
    font-size: 36rpx;
    letter-spacing: 2rpx;
}

.headbox {
    border: 1px #e5e5e5 solid;
    border-radius: 10rpx;
}

.teachername {
    font-size: 34rpx;
    letter-spacing: 2rpx;
    line-height: 80rpx;
    color: #666;
    padding: 0 20rpx;
    display: flex;
    border-bottom: 1px #e5e5e5 solid;
}

.teacherimg {
    width: 40rpx;
    height: 40rpx;
    margin: 20rpx 0;
    margin-right: 20rpx;
}

.teachername:last-child {
    border-bottom: 0px #e5e5e5 solid;
}

.exportbtn {
    background: #19b2ed;
    color: #fff;
    font-size: 30rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    padding: 10rpx 20rpx;
    border-radius: 10rpx;
    letter-spacing: 2rpx;
}

